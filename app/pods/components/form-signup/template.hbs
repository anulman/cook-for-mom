{{yield (hash
  isSubmitted=isSubmitted
  header=(component headerComponent)
  main=(component mainComponent tagName='fieldset')
  footer=(component footerComponent)
  keydown=(action keydown)
  submit=(action submit)
  reset=(action reset)
  thanks=(hash
    header=(component thanksHeaderComponent tagName='')
    main=(component thanksMainComponent tagName='')
    footer=(component thanksFooterComponent tagName='')
  )
)}}
